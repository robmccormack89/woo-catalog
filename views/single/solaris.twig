{% extends 'single.twig' %}
{% import "_snippets.twig" as snippets %}

{% block styles %}
  {{ parent() }}
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Arima+Madurai:wght@300;400;500;700;800;900&family=Source+Sans+Pro:wght@300;400;600;700&display=swap" rel="stylesheet">
  <link rel="stylesheet" href="https://unpkg.com/swiper@8/swiper-bundle.min.css" />
  <script src="https://unpkg.com/swiper@8/swiper-bundle.min.js"></script>
  <style>
  
      /* uikit */
      html {
        font-size: 17px;
      }
      html,
      .uk-navbar-item, .uk-navbar-nav>li>a, .uk-navbar-toggle,
      .uk-logo {
        font-family: 'Source Sans Pro', sans-serif;
      }
      .uk-h1, .uk-h2, .uk-h3, .uk-h4, .uk-h5, .uk-h6, .uk-heading-2xlarge, .uk-heading-large, .uk-heading-medium, .uk-heading-small, .uk-heading-xlarge, h1, h2, h3, h4, h5, h6 {
        font-family: 'Arima Madurai', cursive;
        font-weight: 700;
      }
      .uk-navbar-item, .uk-navbar-nav>li>a, .uk-navbar-toggle {
        font-size: 1rem;
        text-transform: capitalize;;
      }
      .uk-text-lead {
        font-size: 1.3rem;
        line-height: 1.3;
      }
      .uk-button-primary {
        background-color: #131441;
        border: 1px solid transparent;
        color: #fff;
      }
      
      /* new swiper */
      .swiper {
        /* width: 100%;
        height: 100%; */
      }
      .swiper-slide {
        /* text-align: center; */
        /* font-size: 18px;
        background: #fff; */

        /* Center slide text vertically */
        display: -webkit-box;
        display: -ms-flexbox;
        display: -webkit-flex;
        display: flex;
        -webkit-box-pack: center;
        -ms-flex-pack: center;
        -webkit-justify-content: center;
        justify-content: center;
        -webkit-box-align: center;
        -ms-flex-align: center;
        -webkit-align-items: center;
        align-items: center;
      }
      .swiper-slide img {
        /* display: block;
        width: 100%;
        height: 100%;
        object-fit: cover; */
      }
      .swiper-horizontal>.swiper-pagination-bullets, .swiper-pagination-bullets.swiper-pagination-horizontal, .swiper-pagination-custom, .swiper-pagination-fraction {
        bottom: -15px;
      }
      
      /* utility */
      .pull-up-1-1 {
        margin-top: -100%;
      }
      
      .uk-button-small.uk-border-rounded {
        border-radius: 30px;
      }
      .uk-button-large.uk-border-rounded {
        border-radius: 50px;
      }
      
      .padding-left-3 {
        padding-left: 3px;
      }
      
      .uk-button.uk-label-danger:hover,
      .uk-button.uk-label-success:hover {
        color: #fff;
      }
      
      /* responsive */
      @media only screen and (max-width:959px) {
        /* hero section mobile */
        #Hero .pull-up-1-1 {
          margin-top: auto;
        }
        #Hero .content-half {
          height: fit-content;
        }
        #Hero .image-half {
          max-height: 120px;
        }
      }
      

      .uk-light .uk-text-emphasis i {
        color: #cebd9b;
      }
      .uk-label-success.label-default {
        background-color: #00000000;
        color: #999;
        border: 1px solid #b2b2b2;
      }
      .uk-button {
        text-transform: capitalize;
      }
      .uk-light .uk-button-default {
        border-width: 2px;
      }
      
      .pull-up-50 {
        margin-top: -50px;
      }
      .pull-up-15 {
        margin-top: -15px;
      }
      
      .uk-label {
        text-transform: capitalize;
      }
      .label-large {
        padding: 5px 25px;
      }
      
      
      

      .value-message-strip__image-border {
        background-image: url("data:image/svg+xml;utf8,<svg width='63' height='16' fill='none' xmlns='http://www.w3.org/2000/svg'><path d='M63 15.99V0h-2v13.504h-5.39L55.65 7h-1.3v6.504h-4.7V7h-1.3v6.504h-4.7V7h-1.3v6.504h-4.7V7h-1.3v6.504h-5v-10.5L29.65 3v10.504h-5V7h-1.3v6.504h-4.7V7h-1.3v6.504h-4.7V7h-1.3v6.504h-4.7V7h-1.3v6.504H0V16l63-.01z' fill='%23ffffff'/></svg>");
        background-repeat: repeat-x;
        background-position-y: bottom;
        background-size: 40px;
      }
      .value-message-strip__wrapper {
                    padding: 24px;
          position: relative;
          overflow: hidden;
      }
      
      .value-message-strip__items {
        display: flex;
        flex-direction: row;
        justify-content: space-evenly;
      }
      .value-message-strip__item {
        display: flex;
        justify-content: center;
        margin-bottom: 16px;
      }
      .value-message-strip__item-icon {
          flex: 1 1 100px;
      }
      .value-message-strip__item-icon, .value-message-strip__item-text {
          flex: initial;
      }
      .value-message-strip__item-icon {
          height: 60px;
          margin-right: 16px;
          flex: 1 1 100px;
          text-align: right;
      }
      .value-message-strip__item-icon svg {
          height: 100%;
      }
      .value-message-strip__item-text {
          color: #2e2c2c;
          line-height: 1.3;
          letter-spacing: 0;
          /* font-family: ProximaNovaAlt,Arial,"sans-serif"; */
          font-size: 20px;
          font-weight: 700;
          text-transform: none;
          color: inherit;
          text-transform: uppercase;
      }
      
      .value-message-strip__item-text {
          flex: 2 2 200px;
      }
      
      .value-message-strip__tagline {

      }
      
      .value-message-strip__tagline {
          color: #686058;
          line-height: 1.6;
          font-size: 24px;
          font-weight: 500;
          letter-spacing: 3px;
          line-height: 1.5;
          color: inherit;
          text-align: center;
          margin: 0 16px;
      }
  </style>
{% endblock %}

{% block header %}
  <header class="site-header uk-background-default">
    
    <div class="uk-background-primary uk-light padding-10 uk-padding-remove-horizontal">
      <div class="uk-container uk-container-small">
        
        <div class="swiper mySwiper">
          <div class="swiper-wrapper uk-child-width-1-2@s uk-child-width-1-3@m uk-child-width-1-4@l uk-text-primary">
            
            <div class="swiper-slide">
              <div class="uk-text-center uk-text-small">
                <div class="uk-grid uk-grid-collapse uk-child-width-auto uk-flex uk-flex-middle uk-text-emphasis" uk-grid>
                  <div>Price Promise</div>
                  <div class="padding-left-3"><i class="fa-solid fa-euro-sign"></i></div>
                </div>
              </div>
            </div>
            
            <div class="swiper-slide">
              <div class="uk-text-center uk-text-small">
                <div class="uk-grid uk-grid-collapse uk-child-width-auto uk-flex uk-flex-middle uk-text-emphasis" uk-grid>
                  <div>Fully Guaranteed</div>
                  <div class="padding-left-3"><i class="fa-solid fa-clipboard-check"></i></div>
                </div>
              </div>
            </div>
            
            <div class="swiper-slide">
              <div class="uk-text-center uk-text-small">
                <div class="uk-grid uk-grid-collapse uk-child-width-auto uk-flex uk-flex-middle uk-text-emphasis" uk-grid>
                  <div>Rated Excellent</div>
                  <div class="padding-left-3"><i class="fa-solid fa-star"></i></div>
                </div>
              </div>
            </div>
            
            <div class="swiper-slide">
              <div class="uk-text-center uk-text-small">
                <div class="uk-grid uk-grid-collapse uk-child-width-auto uk-flex uk-flex-middle uk-text-emphasis" uk-grid>
                  <div>35 years of value</div>
                  <div class="padding-left-3"><i class="fa-solid fa-calendar-check"></i></div>
                </div>
              </div>
            </div>
          
          </div>
        </div>
        
      </div>
    </div>
    
    <nav class="logo-nav uk-background-muted padding-10 uk-padding-remove-horizontal">
      <div class="uk-container">
        <ul class="uk-flex uk-flex-middle" uk-grid>
          <li class="uk-width-auto">
            <a class="uk-logo uk-text-bold" href="{{ site.link|e('esc_url') }}">
              {% if theme.logo %}
                <img src="{{ theme.logo.src|e('esc_url') }}" width="{{ theme.logo.w }}" height="{{ theme.logo.h }}" alt="{{ theme.logo.alt|e('esc_html') }}">
              {% else %}
                {{ site.title|e('esc_html') }}
              {% endif %}
            </a>
        	</li>
          <li class="uk-width-expand uk-flex uk-flex-center uk-text-center uk-visible@m">
            Call 1800 34 11 66 to request an appointment with our team
          </li>
          <li class="uk-width-auto uk-flex uk-flex-right uk-text-right uk-visible@m">
            <div>
              <a class="uk-button uk-label-danger uk-button-small uk-border-rounded">Freephone <i class="fa-solid fa-phone-flip padding-left-3"></i></a>
              <a class="uk-button uk-label-success uk-button-small uk-border-rounded">Whatsapp <i class="fa-brands fa-whatsapp padding-left-3"></i></a>
              <a class="uk-button uk-label-success label-default uk-button-small uk-border-rounded">Request a Brochure <i class="fa-solid fa-download padding-left-3"></i></a>
            </div>
          </li>
        </ul>
      </div>
    </nav>
    
    <nav class="main-menu-nav padding-10 uk-padding-remove-horizontal theme-border-top theme-border-bottom uk-visible@m">
